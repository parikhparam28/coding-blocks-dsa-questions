#include<iostream>
#include<bits/stdc++.h>
using namespace std;

void solve()
{
	int n;
	cin>>n;
	vector<int>v(n);
	for(int i=0;i<n;i++)
		cin>>v[i];
	int sum=0,mx=0;
	for(int i=0;i<n;i++)
	{
		sum+=v[i];
		if(sum<0) sum=0;
		mx=max(mx,sum);
	}
	cout<<mx<<endl;
}

int main() {
	int t;
	cin>>t;
	while(t--)
	{
		solve();
	}
	return 0;
}
